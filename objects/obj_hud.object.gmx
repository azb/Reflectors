            <string>
line_dis = 400

scr_game_end()

//draw_text(mouse_x,mouse_y,obj_ball.right_key_pressed)

w = sprite_get_width(spr_arrow)

for(i=0 ; i &lt; 4 ; i++)
    {
    dir = 0
    switch(i)
        {
        case 0:
            dir = 180
        break;
        case 1:
            dir = 90
        break;
        case 2:
            dir = 270
        break;
        case 3:
            dir = 0
        break;
        }
    draw_sprite_ext(spr_arrow,0,w/2 + i * 96 + 48,w/2,1,1,dir,c_white,1)
    draw_sprite_ext(spr_arrow,0,room_width - w/2 - 96 * 4 + i * 96 + 48,w/2,1,1,dir,c_white,1)
    
    if obj_ball.right_key_pressed1 = 2 || obj_player1.alarm[0] &gt; 1
        {
        draw_sprite_ext(spr_arrow,1,w/2 + i * 96 + 48,w/2,1,1,dir,c_red,sin(degtorad(obj_player1.alarm[0] * 60)))
        //draw_rectangle(0,0,room_width,room_height,0)
        }
    if obj_ball.right_key_pressed2 = 2 || obj_player2.alarm[0] &gt; 1
        {
        draw_sprite_ext(spr_arrow,1,room_width - w/2 - 96 * 4 + i * 96 + 48,w/2,1,1,dir,c_red,sin(degtorad(obj_player2.alarm[0] * 60)))
        }
    }

xd = 0
switch(image_angle / 90)
    {
    case 0:
        xd = 3
    break;
    case 1:
        xd = 1
    break;
    case 2:
        xd = 0
    break;
    case 3:
        xd = 2
    break;
    }

if obj_ball.x &lt; line_dis
if obj_ball.side = -1
    {
    draw_sprite_ext(spr_arrow,1,w/2 + xd * 96 + 48,w/2,1,1,image_angle,c_white,1)
    }

if obj_ball.x &gt; room_width - line_dis
if obj_ball.side = 1
    {
    draw_sprite_ext(spr_arrow,1,room_width - w/2 - 96 * 4 + xd * 96 + 48,w/2,1,1,image_angle,c_white,1)
    }
    
for(i=0 ; i &lt; 3 ; i++)
    {
    draw_sprite(spr_lives,1,24,24 + i * 32)
    draw_sprite(spr_lives,1,room_width - 24,24 + i * 32)
    
    if p1_lives &gt; i
        draw_sprite(spr_lives_animation,image_index + i,24,24 + i * 32)
    if p2_lives &gt; i
        draw_sprite(spr_lives_animation,image_index + i,room_width - 24,24 + i * 32)
    }
    
if obj_ball.alarm[0] &gt; 0
    {
    draw_set_halign(fa_center)
    draw_set_valign(fa_center)
    draw_set_font(fnt_getready)
    draw_set_color(c_white)
    if !obj_ball.game_over
    draw_text(room_width/2,room_height/2 - 32,"GET READY")
    else
    draw_text(room_width/2,room_height/2 - 32,"GAME OVER!")
    
    if firsttime
        {
        draw_set_font(fnt_countdown)
        if obj_ball.alarm[0] &gt;= 16
            {
            draw_set_color(c_white)
            if round((obj_ball.alarm[0]-10) / 30) != 0
            draw_text(room_width/2,room_height/2 + 64,round((obj_ball.alarm[0]-10) / 30))
            else
            draw_text(room_width/2,room_height/2 + 64,1)
            }
        else
            {
            draw_set_color(c_white)
            draw_text(room_width/2,room_height/2 + 64,"GO!")
            }
        //if round(obj_ball.alarm[0] / 30) = obj_ball.alarm[0] / 30
    
        if obj_ball.alarm[0] = 89 + 20
        sound_play(snd_countdown_1)
        if obj_ball.alarm[0] = 60 + 20
        sound_play(snd_countdown_1)
        if obj_ball.alarm[0] = 30 + 20
        sound_play(snd_countdown_1)
        if obj_ball.alarm[0] = 1 + 20
            {
            sound_play(snd_countdown_go)
            }
        if obj_ball.alarm[0] = 1
            firsttime = 0
        }
    }

